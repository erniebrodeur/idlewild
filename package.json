{
  "name": "idlewild",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "dev:nocache": "vite --force --host",
    "build": "vite build",
    "build:watch": "chokidar 'src/**/*' -c \"npm run build\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "livereload": "concurrently \"npm run build:watch\" \"live-server --root=dist --port=5173 --watch=dist --no-browser\"",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --host"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^5.0.0",
    "globals": "^16.4.0",
    "typescript": "^5.4.2",
    "vite": "^7.1.7",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.0",
    "live-server": "^1.2.2"
  }
}
